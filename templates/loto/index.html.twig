{% extends 'base.html.twig' %}

{% block title %}Loto index{% endblock %}

{% block body %}
    <h1>Loto index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Annee_numero_de_tirage</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for loto in lotos %}
            <tr>
                <td>{{ loto.id }}</td>
                <td>{{ loto.anneeNumeroDeTirage }}</td>
                <td>
                    <a href="{{ path('loto_show', {'id': loto.id}) }}">show</a>
                    <a href="{{ path('loto_edit', {'id': loto.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('loto_new') }}">Create new</a>
{% endblock %}
